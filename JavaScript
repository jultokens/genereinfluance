async function generateImage() {
    const response = await fetch('https://api.dallemini.com/generate ', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({
            prompt: `Sexy influencer with ${selectedEthnicity}, ${selectedEyeColor} eyes, ${selectedHairLength} hair, wearing a ${selectedOutfit}`
        })
    });
    const data = await response.json();
    return data.image_url;
}
